{
  "dashboard": {
    "id": null,
    "title": "RecoveryOS Elite AI Endpoints",
    "tags": ["recoveryos", "elite-ai", "production"],
    "timezone": "UTC",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate (RPS)",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(recoveryos_requests_total[5m])",
            "legendFormat": "{{endpoint}} - {{status}}"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Response Time Percentiles",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(recoveryos_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P50 - {{endpoint}}"
          },
          {
            "expr": "histogram_quantile(0.95, rate(recoveryos_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 - {{endpoint}}"
          },
          {
            "expr": "histogram_quantile(0.99, rate(recoveryos_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P99 - {{endpoint}}"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(recoveryos_errors_total[5m]) / rate(recoveryos_requests_total[5m]) * 100",
            "legendFormat": "Error Rate % - {{endpoint}}"
          }
        ],
        "yAxes": [
          {
            "label": "Error Rate %",
            "min": 0,
            "max": 100
          }
        ],
        "alert": {
          "conditions": [
            {
              "query": {"queryType": "", "refId": "A"},
              "reducer": {"type": "last", "params": []},
              "evaluator": {"params": [1], "type": "gt"}
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "10s",
          "handler": 1,
          "name": "High Error Rate Alert",
          "noDataState": "no_data",
          "notifications": []
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Active Requests",
        "type": "graph",
        "targets": [
          {
            "expr": "recoveryos_active_requests",
            "legendFormat": "Active - {{endpoint}}"
          }
        ],
        "yAxes": [
          {
            "label": "Count",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Elite Endpoints Status",
        "type": "table",
        "targets": [
          {
            "expr": "recoveryos_requests_total",
            "format": "table",
            "instant": true
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Canary Deployment Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"recoveryos\"}",
            "legendFormat": "Service Up"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 24}
      },
      {
        "id": 7,
        "title": "Feature Flag Status",
        "type": "stat",
        "targets": [
          {
            "expr": "recoveryos_feature_flag_enabled{flag=\"release_20250814\"}",
            "legendFormat": "Release Flag"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 24}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}
